{
    "arrow_spacing": {
        "level": "warn"
    },
    "braces_spacing": {
        "empty_object_spaces": 0,
        "level": "ignore",
        "spaces": 0
    },
    "camel_case_classes": {
        "level": "error"
    },
    "check_scope": {
        "environments": [
            "es5",
            "es6",
            "node"
        ],
        "globals": {
            "$": true,
            "jQuery": true
        },
        "hoist_local": true,
        "hoist_parent": true,
        "level": "warn",
        "module": "coffeescope2",
        "overwrite": true,
        "shadow": true,
        "shadow_builtins": false,
        "shadow_exceptions": [
            "err",
            "next"
        ],
        "undefined": true,
        "unused_arguments": false,
        "unused_classes": true,
        "unused_variables": true
    },
    "coffeescript_error": {
        "level": "error"
    },
    "colon_assignment_spacing": {
        "level": "ignore",
        "spacing": {
            "left": 0,
            "right": 0
        }
    },
    "cyclomatic_complexity": {
        "level": "ignore",
        "value": 10
    },
    "duplicate_key": {
        "level": "error"
    },
    "empty_constructor_needs_parens": {
        "level": "ignore"
    },
    "ensure_comprehensions": {
        "level": "warn"
    },
    "eol_last": {
        "level": "ignore"
    },
    "indentation": {
        "level": "error",
        "value": 1
    },
    "line_endings": {
        "level": "ignore",
        "value": "unix"
    },
    "max_line_length": {
        "level": "warn",
        "limitComments": true,
        "value": 120
    },
    "missing_fat_arrows": {
        "is_strict": false,
        "level": "ignore"
    },
    "newlines_after_classes": {
        "level": "ignore",
        "value": 3
    },
    "no_backticks": {
        "level": "error"
    },
    "no_debugger": {
        "console": false,
        "level": "warn"
    },
    "no_empty_functions": {
        "level": "ignore"
    },
    "no_empty_param_list": {
        "level": "ignore"
    },
    "no_implicit_braces": {
        "level": "ignore",
        "strict": true
    },
    "no_implicit_parens": {
        "level": "ignore",
        "strict": true
    },
    "no_interpolation_in_single_quotes": {
        "level": "ignore"
    },
    "no_nested_string_interpolation": {
        "level": "warn"
    },
    "no_plusplus": {
        "level": "ignore"
    },
    "no_private_function_fat_arrows": {
        "level": "warn"
    },
    "no_stand_alone_at": {
        "level": "ignore"
    },
    "no_tabs": {
        "level": "ignore"
    },
    "no_this": {
        "level": "warn"
    },
    "no_throwing_strings": {
        "level": "error"
    },
    "no_trailing_semicolons": {
        "level": "error"
    },
    "no_trailing_whitespace": {
        "allowed_in_comments": false,
        "allowed_in_empty_lines": true,
        "level": "error"
    },
    "no_unnecessary_double_quotes": {
        "level": "ignore"
    },
    "no_unnecessary_fat_arrows": {
        "level": "warn"
    },
    "non_empty_constructor_needs_parens": {
        "level": "ignore"
    },
    "prefer_english_operator": {
        "doubleNotLevel": "ignore",
        "level": "ignore"
    },
    "space_operators": {
        "level": "ignore"
    },
    "spacing_after_comma": {
        "level": "ignore"
    },
    "transform_messes_up_line_numbers": {
        "level": "warn"
    }
}
